<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <navigation-case>
            <to-view-id>privado/index.jsf</to-view-id>
            <from-outcome>index</from-outcome>
        </navigation-case>

        <!--secretaria-->
        <navigation-case>
            <to-view-id>secretaria/local/lista_local.jsf</to-view-id>
            <from-outcome>lista_local</from-outcome>
        </navigation-case>
        <navigation-case>
            <to-view-id>secretaria/local/form_local.jsf</to-view-id>
            <from-outcome>form_local</from-outcome>
        </navigation-case>
        <navigation-case>
            <to-view-id>secretaria/proprietario/lista_proprietario.jsf</to-view-id>
            <from-outcome>lista_proprietario</from-outcome>
        </navigation-case>
        <navigation-case>
            <to-view-id>secretaria/proprietario/form_proprietario.jsf</to-view-id>
            <from-outcome>form_proprietario</from-outcome>
        </navigation-case>
        <navigation-case>
            <to-view-id>secretaria/analise/lista_analise.jsf</to-view-id>
            <from-outcome>lista_analise</from-outcome>
        </navigation-case>
        <navigation-case>
            <to-view-id>secretaria/analise/form_analise.jsf</to-view-id>
            <from-outcome>form_analise</from-outcome>
        </navigation-case>
        <navigation-case>
            <to-view-id>secretaria/solicitacao/form_solicitacao.jsf</to-view-id>
            <from-outcome>form_solicitacao</from-outcome>
        </navigation-case>
        <navigation-case>
            <to-view-id>secretaria/solicitacao/lista_solicitacao.jsf</to-view-id>
            <from-outcome>lista_solicitacao</from-outcome>
        </navigation-case>
        <navigation-case>
            <to-view-id>secretaria/solicitacao/mostrar_solicitacao.jsf</to-view-id>
            <from-outcome>lista_solicitacao</from-outcome>
        </navigation-case>
        <!--fim secretaria-->
        
        <navigation-case>
            <to-view-id>admin/usuario/form_usuario.jsf</to-view-id>
            <from-outcome>form_usuario</from-outcome>
        </navigation-case>
        <navigation-case>
            <to-view-id>admin/usuario/lista_usuario.jsf</to-view-id>
            <from-outcome>lista_usuario</from-outcome>
        </navigation-case>
        
    </navigation-rule>
</faces-config>
